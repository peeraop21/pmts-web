@model PMTs.DataAccess.ModelView.MaintenanceFlute.MaintenanceFluteModel

@{ int No = 1; }

@foreach (var item in Model.Flutes)
{
        <tr>
            <th scope="row">@No</th>
            <td>@item.Flute1</td>
            <td>@item.Code</td>
            <td>@item.Height</td>
            <td>@item.Layer</td>

            <td align="center">
                <a><i class="fas fa-file text-info" data-id1="@item.Flute1"  onclick="viewEditDataView(this.getAttribute('data-id1'))"></i></a>
            </td>
            <td align="center">
                <a> <i class="fas fa-pencil-alt text-warning" data-id1="@item.Flute1"  onclick="viewEditData(this.getAttribute('data-id1'))"></i></a>
            </td>
        </tr>
        No++;
 }