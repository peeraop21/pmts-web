<body style="margin:auto;padding-top:10px">
    @{
        var numberOfMasterCard = 1;
        var Orderitem = "";

    }
    <div id="pageMasterCard" class="container" style="width: 1510px;padding: 0px;">

        @foreach (var item in Model.MasterCardMOs)
        {
            Orderitem = item.OrderItem;
            <div id="printMain" style="min-height:2125px;width:1510px;margin:0">

                @*part1 Icon,qrCode,barCode,Doc desc*@
                <div class="row" style="width: 1550px; padding-top: 10px">
                    <div class="col-xs-8 row">
                        <div class="col-xs-2">
                            <img src="~/pic/Logo_SCG.jpg" width="120" height="120" />
                        </div>
                        @if (item.IsPreview)
                        {
                            <div class="col-xs-10" style="min-height: 135px;">
                                <font style="letter-spacing: 3px; padding-left: 20px; font-size:75px;text-shadow: 2px 2px #C0C0C0; color: #808080; font-weight: 700; ">@Localizer["PREVIEW MODE"]</font>
                            </div>
                        }
                        else
                        {

                            <div class="col-xs-10" style="padding-top:5px">
                                <div class="row ">
                                    <div class="col-xs-7">
                                        <div class="col-md-12">
                                            <font class="FontSubHeader2">@item.Factory</font>
                                        </div>

                                        <div class="col-md-12">
                                            @if (item.OrderItem == "" || item.OrderItem == null)
                                            {
                                                <font class="FontHeader">@Localizer["Master Card"]</font>

                                            }
                                            else
                                            {
                                                <font class="FontHeader">@Localizer["Master Card M/O"]</font>
                                            }
                                        </div>
                                    </div>
                                    <div class="col-xs-4 frame">
                                        @*<div class="divbo" id="qrcode_@numberOfMasterCard"></div>*@
                                        @if (!string.IsNullOrEmpty(item.OrderItem))
                                        {
                                            <div style="font-weight: 700; text-align: left; border: none; font-size: 24px; position: relative; top: 0; left: -5%; margin: 0; white-space: nowrap; padding: 5px 10px; width: auto;">@Localizer["PO No."]&nbsp;@item.PoNo</div>
                                        }
                                    </div>
                                </div>
                            </div>
                        }
                    </div>

                    <script src="~/lib/Barcode/JsBarcode.all.min.js"></script>

                    <div class="col-xs-1" style="max-width:1px">
                        <div style="position:absolute;top: 10px;font-weight:700;text-align: right;width: 100px;border:none;font-size: 42px;right:-30px;">@item.Flute</div>
                    </div>
                    <div class="col-xs-3" style="float:right;min-width:366.66px"></div>

                    <!--#region Barcode -->
                    <div class="col-xs-3" style="float:right;min-width:366.66px">
                        <div class="text-center row">

                            <div id="barcodePadding" class="col-xs-12">
                                @if (item.MoRout.Count() > 0)
                                {
                                    if (item.OrderItem != "" && item.OrderItem != null)
                                    {
                                        <svg class="barcode" style="height:85px; width:400px;padding:0;margin:0"
                                             jsbarcode-format="CODE39"
                                             jsbarcode-value="@item.OrderItem"
                                             jsbarcode-textmargin="0">
                                        </svg>
                                    }
                                    else
                                    {
                                        <div class="col-md-12" style="min-height:55px;padding-top:55px;">&nbsp;</div>
                                    }
                                }
                                else if (item.Rout.Count() > 0)
                                {
                                    if (item.EanCode != "" && item.EanCode != null)
                                    {
                                        <svg class="barcode" style="height:85px; width:400px;padding:0;margin:0"
                                             jsbarcode-format="CODE39"
                                             jsbarcode-value="@item.EanCode"
                                             jsbarcode-textmargin="0">
                                        </svg>
                                    }
                                    else
                                    {
                                        <div class="col-md-12" style="min-height:55px;padding-top:55px;">&nbsp;</div>
                                    }
                                }
                                else
                                {
                                    <div class="col-md-12" style="min-height:55px;padding-top:55px;">&nbsp;</div>
                                }
                            </div>

                            <script type="text/javascript">
                                JsBarcode(".barcode").init();
                            </script>
                            <div class="col-xs-2"></div>
                            <div class="col-xs-5">

                                <font>@item.DocName</font>
                            </div>
                            <div class="col-xs-1"></div>
                            <div class="col-xs-4">

                                <font>@item.DocDate</font>
                            </div>
                        </div>
                    </div>
                    <!--#endregion -->
                </div>

                @*part2 Product desc,Image , Routing*@
                <table style="margin-right: 0; border-bottom: none; border-top: none;" border="1">
                    <tbody>
                        <tr>
                            @*Product desc*@
                            <td class="row" style="width: 882px; border-bottom: 1px solid black; min-height: 1420px; max-height: 1420px; vertical-align: top;" height="1420px;">

                                <div class="col-xs-12" style="padding: 0px; height: 180px; width: 100%; border-bottom: 1px solid black;">
                                    <div>
                                        <font class="FontCustomer"> @item.Cust_Name</font>
                                    </div>
                                    <div>
                                        <font class="FontSubHeader1"> @item.Description</font>
                                    </div>
                                    <div>
                                        <font class="FontSubHeader2"> @item.Sale_Text1</font>
                                    </div>
                                </div>

                                <table style="width: 882px;" class="tableMO" cellspacing="0" cellpadding="0">
                                    <tbody>

                                        @*H:65*@
                                        <tr style="height: 65px;">
                                            <td class="tableMO" colspan="3" style="width: 85px;border-left-color:white;">
                                                <font class="FontTitle">@Localizer["Product Code"]</font>
                                                <div class="text-center">
                                                    <b><font class="FontParagraph text-wrap" style="font-size:26px">@item.PC</font></b>
                                                </div>
                                            </td>
                                            @*85*@
                                            <td class="tableMO" colspan="4" style="width: 85px;border-top-color:white">
                                                <font class="FontTitle">@Localizer["Material No."]</font>
                                                <div class="text-center">
                                                    <b><font class="FontParagraph" style="font-size:30px;letter-spacing:5px">@item.Material_No</font></b>
                                                </div>
                                            </td>
                                            @*85*@
                                        </tr>
                                        @*H:65*@
                                        <tr style="height: 65px;">
                                            <td class="tableMO" colspan="3" style="width: 85px;border-left-color:white">
                                                <font class="FontParagraph">@item.Box_Type</font>
                                                <div>
                                                    <font class="FontTitle">@Localizer["รูปแบบ"] : </font>
                                                    <font class="FontParagraph">@item.RSC_Style  &nbsp; @item.CGType</font>
                                                </div>
                                            </td>
                                            <td class="tableMO" colspan="4" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["Part No"].</font>
                                                <div class="text-center">
                                                    <b><font class="FontParagraph" style="font-weight:normal">@item.Part_No</font></b>
                                                </div>
                                            </td>
                                        </tr>
                                        @*H:65*@
                                        <tr style="height: 65px;">
                                            <td class="tableMO" rowspan="2" style="width: 85px;border-left-color:white">
                                                @if (@item.PalletPath_Base64 != null && @item.PalletPath_Base64 != "")
                                                {
                                                    <div class="frame" style="width: 65px;max-height:65px">
                                                        <img class="imageFrame" src="@item.PalletPath_Base64" />
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="frame" style="width: 65px;max-height:100px">&nbsp;</div>
                                                }
                                            </td>
                                            <td class="tableMO" colspan="2" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["รอยต่อ"] : </font>
                                                <b><font class="FontParagraph" style="font-weight:normal">@item.JoinType</font></b>
                                            </td>
                                            <td class="tableMO" colspan="2" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["การพิมพ์"] : </font>
                                                <b><font class="FontParagraph" style="font-weight:normal">@item.Print_Method</font></b>
                                            </td>
                                            <td class="tableMO text-center" colspan="2">
                                                @if (item.GlWid.Value)
                                                {
                                                    <font class="text-center FontParagraph" style="font-weight:normal">@Localizer["ลิ้นกาวด้านกว้าง"]</font>
                                                }
                                                <font class=" text-center" size="3"></font>
                                            </td>
                                        </tr>
                                        @*H:80*@
                                        <tr style="height: 65px;">
                                            <td class="tableMO" style="width: 100px;width: 85px;">
                                                <font class="FontTitle">@Localizer["ขนาดกระบะ"]</font>
                                                <div class="text-center">
                                                    @if (item.PalletSize != null && item.PalletSize != "")
                                                    {
                                                        <b><font class="FontParagraph">@item.PalletSize</font></b>
                                                    }
                                                    else
                                                    {
                                                        <font class="FontParagraph">&nbsp;</font>
                                                    }
                                                </div>
                                            </td>
                                            <td class="tableMO" style="width: 100px;width: 85px;">
                                                <font class="FontTitle">@Localizer["กล่อง/มัด"]</font>
                                                <div class="text-center">
                                                    @if (item.Bun.HasValue)
                                                    {
                                                        <b><font class="FontParagraph">@item.Bun</font></b>
                                                    }
                                                    else
                                                    {
                                                        <font class="FontParagraph">&nbsp;</font>
                                                    }
                                                </div>
                                            </td>
                                            <td class="tableMO" style="width: 85px;">
                                                <font class="FontTitle" size="2">@Localizer["เรียง(มัด/ชั้น)"]</font>
                                                <div class="text-center">
                                                    @if (item.BunLayer.HasValue)
                                                    {
                                                        <b><font class="FontParagraph">@item.BunLayer</font></b>
                                                    }
                                                    else
                                                    {
                                                        <font class="FontParagraph">&nbsp;</font>
                                                    }
                                                </div>
                                            </td>
                                            <td class="tableMO" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["สูง(ชั้น/กระบะ)"]</font>
                                                <div class="text-center">
                                                    <b><font class="FontParagraph">@item.LayerPalet</font></b>
                                                </div>
                                            </td>
                                            <td class="tableMO" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["กล่อง/กระบะ"]</font>
                                                <div class="text-center">
                                                    <b><font class="FontParagraph">@item.BoxPalet</font></b>
                                                </div>
                                            </td>
                                            <td class="tableMO" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["ประเภทสินค้า"]</font>
                                                <div class="text-center">
                                                    <b><font class="FontParagraph">@item.Material_Type</font></b>
                                                </div>
                                            </td>
                                        </tr>
                                        @*H:80*@
                                        <tr style="height: 65px;">
                                            <td class="tableMO" style="width: 85px;border-left-color:white">
                                                <font class="FontTitle">@Localizer["การผลิต"]</font>
                                                <div class="text-center">
                                                    <b><font class="FontParagraph">@item.Status_Flag</font></b>
                                                </div>
                                            </td>
                                            <td class="tableMO" style="width: 85px;"></td>
                                            <td class="tableMO" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["ชิ้น/แพ็ค"]</font>
                                                <div class="text-center">
                                                    @if (item.Piece_Patch != null)
                                                    {
                                                        <b><font class="FontParagraph">@item.Piece_Patch.Value</font></b>
                                                    }
                                                    else
                                                    {
                                                        <b><font class="FontParagraph">&nbsp;</font></b>
                                                    }
                                                </div>
                                            </td>
                                            <td class="tableMO" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["ชิ้น/ชุด"]</font>
                                                <div class="text-center">
                                                    <b><font class="FontParagraph">@item.Piece_Set</font></b>
                                                </div>
                                            </td>
                                            <td class="tableMO" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["ตัวลวด/กล่อง"]</font>
                                                <div class="text-center">
                                                    @if (item.Wire.HasValue)
                                                    {
                                                        <b><font class="FontParagraph">@item.Wire</font></b>
                                                    }
                                                    else
                                                    {
                                                        <font class="FontParagraph">&nbsp;</font>
                                                    }
                                                </div>
                                            </td>

                                            @if (item.TwoPiece.Value)
                                            {
                                                <td class="tableMO text-center" colspan="2" style="width: 85px;" row>
                                                    <div style="min-height:23px"><b>@Localizer["2 ชิ้นต่อ"]</b></div>
                                                    <div style="border-top-color:black;border-top:1px solid black;height:25px;">
                                                        <div style="margin-top: 5px;">
                                                            <b><font class="FontTitle">@item.Hierarchy</font></b>
                                                        </div>
                                                    </div>
                                                </td>
                                            }
                                            else
                                            {
                                                <td class="tableMO text-center" colspan="2" style="width: 85px;" row>
                                                    <div style="min-height:23px"></div>
                                                    <div style="border-top-color: black; border-top: 1px solid black; height: 25px;">
                                                        <div style="margin-top: 5px;">
                                                            <b><font class="FontTitle" style="">@item.Hierarchy</font></b>
                                                        </div>
                                                    </div>
                                                </td>
                                            }
                                        </tr>
                                        @*H:80*@
                                        <tr style="height: 65px;">
                                            <td class="tableMO" colspan="2" style="width: 85px;border-left-color:white">
                                                <font class="FontTitle" style="padding-bottom:15px">@Localizer["มิติภายใน (ย x ก x ส)"]</font>

                                                @if (item.Leg != null || @item.Wid != null || @item.Hig != null)
                                                {
                                                    @if (item.Leg.HasValue || @item.Wid.HasValue || @item.Hig.HasValue)
                                                    {
                                                        @if (item.Leg.Value != null || @item.Wid.Value != null || @item.Hig.Value != null)
                                                        {
                                                            if (@item.Hig == null)
                                                            {
                                                                <div class=" text-center">
                                                                    <b><font class=" text-right FontParagraph">@item.Leg x @item.Wid</font></b>
                                                                </div>
                                                            }
                                                            else
                                                            {
                                                                <div class=" text-center">
                                                                    <b><font class=" text-right FontParagraph">@item.Leg x @item.Wid x @item.Hig</font></b>
                                                                </div>
                                                            }
                                                        }
                                                    }
                                                    else
                                                    {
                                                        <div>
                                                            <b><font class=" text-right FontParagraph">&nbsp;</font></b>
                                                        </div>
                                                    }
                                                }
                                                else
                                                {
                                                    <div>
                                                        <b><font class=" text-right FontParagraph">&nbsp;</font></b>
                                                    </div>
                                                }
                                            </td>
                                            <td class="tableMO" colspan="2" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["ขนาดตัด (ก x ย)"]</font>
                                                <div class="text-center">
                                                    @if (@item.CutSheetWid.HasValue && @item.CutSheetLeng.HasValue)
                                                    {
                                                        @if (@item.CutSheetWid.Value != null && @item.CutSheetLeng.Value != null)
                                                        {
                                                            <b> <font class="FontParagraph">@item.CutSheetWid x @item.CutSheetLeng</font></b>
                                                        }
                                                    }
                                                    else
                                                    {
                                                        <b> <font class="FontParagraph">&nbsp;</font></b>
                                                    }
                                                </div>
                                            </td>
                                            <td class="tableMO" colspan="2" style="width: 85px;">
