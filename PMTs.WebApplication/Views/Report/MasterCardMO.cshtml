<body style="margin:auto;padding-top:10px">
    @{
        var numberOfMasterCard = 1;
        var Orderitem = "";

    }
    <div id="pageMasterCard" class="container" style="width: 1068px;padding: 0px;">

        @foreach (var item in Model.MasterCardMOs)
        {
            Orderitem = item.OrderItem;
            <div id="printMain" style="min-height:1480px;width:1068px;margin:0">

                <div class="row" style="width:1100px;padding-top:10px">
                    <div class="col-xs-8 row">
                        <div class="col-xs-2">
                            <img src="~/pic/Logo_SCG.jpg" width="90" height="90" />
                        </div>
                        @if (item.IsPreview)
                        {
                            <div class="col-xs-10" style="min-height: 105px;">
                                <font style="letter-spacing: 3px; padding-left: 20px; font-size:67px;text-shadow: 2px 2px #C0C0C0; color: #808080; font-weight: 700; ">@Localizer["PREVIEW MODE"]</font>
                            </div>
                        }
                        else
                        {

                            <div class="col-xs-10" style="padding-top:5px">
                                <div class="row ">
                                    <div class="col-xs-7">
                                        <div class="col-md-12">
                                            <font class="FontSubHeader2">@item.Factory</font>
                                        </div>

                                        <div class="col-md-12">
                                            @if (item.OrderItem == "" || item.OrderItem == null)
                                            {
                                                <font class="FontHeader">@Localizer["Master Card"]</font>

                                            }
                                            else
                                            {
                                                <font class="FontHeader">@Localizer["Master Card M/O"]</font>
                                            }
                                        </div>
                                    </div>
                                    <div class="col-xs-4" style="float:right">
                                        <div style="padding-bottom:5px;padding-left:30px">
                                            <div class="divbo" id="qrcode_@numberOfMasterCard"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>

                    <script src="~/lib/Barcode/JsBarcode.all.min.js"></script>

                    <div class="col-xs-1" style="max-width:1px">
                        <div style="position:absolute;top: 10px;font-weight:700;text-align: right;width: 100px;border:none;font-size: 42px;right:-30px;">@item.Flute</div>
                    </div>

                    <div class="col-xs-3" style="float:right;min-width:366.66px">
                        <div class="text-center row">

                            <div id="barcodePadding" class="col-xs-12">
                                @if (item.MoRout is not null && item.MoRout.Count() > 0)
                                {
                                    if (item.OrderItem != "" && item.OrderItem != null)
                                    {
                                        <svg class="barcode" style="height:85px; width:400px;padding:0;margin:0"
                                             jsbarcode-format="CODE39"
                                             jsbarcode-value="@item.OrderItem"
                                             jsbarcode-textmargin="0">
                                        </svg>
                                    }
                                    else
                                    {
                                        <div class="col-md-12" style="min-height:55px;padding-top:55px;">&nbsp;</div>
                                    }
                                }
                                else if (item.Rout is not null && item.Rout.Count() > 0)
                                {
                                    if (item.EanCode != "" && item.EanCode != null)
                                    {
                                        <svg class="barcode" style="height:85px; width:400px;padding:0;margin:0"
                                             jsbarcode-format="CODE39"
                                             jsbarcode-value="@item.EanCode"
                                             jsbarcode-textmargin="0">
                                        </svg>
                                    }
                                    else
                                    {
                                        <div class="col-md-12" style="min-height:55px;padding-top:55px;">&nbsp;</div>
                                    }
                                }
                                else
                                {
                                    <div class="col-md-12" style="min-height:55px;padding-top:55px;">&nbsp;</div>
                                }
                            </div>

                            <script type="text/javascript">
                                JsBarcode(".barcode").init();
                            </script>
                            <div class="col-xs-2"></div>
                            <div class="col-xs-5">

                                <font>@item.DocName</font>
                            </div>
                            <div class="col-xs-1"></div>
                            <div class="col-xs-4">

                                <font>@item.DocDate</font>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="lineMO " style="width:1067px;border-bottom:none;">
                    <div class="row" style="min-height: 160px;">
                        <div class="col-xs-7" style="padding-right:0px">
                            <div>
                                <font class="FontCustomer"> @item.Cust_Name</font>
                            </div>
                            <div>
                                <font class="FontSubHeader1"> @item.Description</font>
                            </div>
                            <div>
                                @*<font class="FontSubHeader2"> @item.Sale_Text1</font>*@
                                @if (item.OrderItem == "" || item.OrderItem == null)
                                {
                                    <font class="FontSubHeader2"> @item.Sale_Text1</font>
                                }
                                else
                                {
                                    if (item.Material_No.StartsWith("Z02"))
                                    {
                                        <font class="FontSubHeader2"> @item.Sale_Text1 SKIC</font>
                                    }
                                    else
                                    {
                                        <font class="FontSubHeader2"> @item.Sale_Text1</font>
                                    }
                                    @*<font class="FontHeader">@Localizer["Master Card M/O"]</font>*@
                                }
                            </div>

                            @if (!string.IsNullOrEmpty(item.PoNo))
                            {
                                <div style="float:right;max-width:1px;">
                                    <div class="FontParagraph" style="position:absolute;top: 135px;text-align: right;width: 280px;border:none;right:-70px;">@Localizer["PO No"]:&nbsp;@item.PoNo</div>
                                </div>
                            }
                        </div>
                        <div class="col-xs-5 pull-right text-center" style="padding-left:8px;padding-right:0px">
                            <table class="text-center tableMO" style="border-bottom:none">
                                <tbody>
                                    <tr style="height:35px">
                                        <td class="tableMO" style="width:17%;">@Localizer["จน.ผลิต(แผ่น)"]</td>
                                        <td class="tableMO" style="width:15%;">@Localizer["จน.ตัด"]</td>
                                        <td class="tableMO" style="width:15%;">@Localizer["กว้าง"]</td>
                                        <td class="tableMO" style="width:15%;">@Localizer["ตัด"]</td>
                                        <td class="tableMO" style="width:15%;">@Localizer["เศษ"]</td>
                                        <td class="tableMO" style="width:17%;">@Localizer["ยาวเมตร"]</td>
                                    </tr>
                                    <tr style="height:35px">
                                        @if (@item.MoRout is not null && @item.MoRout.Count() > 0)
                                        {
                                            if (!string.IsNullOrEmpty(item.Target_Quant))
                                            {
                                                <td class="boarderLeft"> <font>@String.Format("{0:#,#}", float.Parse(item.Target_Quant))</font></td>
                                            }
                                            else
                                            {
                                                <td class="boarderLeft"><font></font></td>
                                            }

                                            if (@item.CutNo != "")
                                            {
                                                <td class="boarderLeft" style="border-right:1px solid black;"> <font>@String.Format("{0:#,#}", float.Parse(item.CutNo))</font></td>
                                                @if (item.MoRout.Where(x => x.IsProp_Cor == true).Count() > 0)
                                                {
                                                    @foreach (var x in item.MoRout)
                                                    {
                                                        if (x.IsProp_Cor)
                                                        {
                                                            @if (x.Routing.PaperWidth != null && (x.Routing.PaperWidth != 0 || !x.Routing.PaperWidth.ToString().Contains("0.")))
                                                            {
                                                                <td class="boarderLeft" style="border-right:1px solid black;"><font>@String.Format("{0:#}", float.Parse(x.Routing.PaperWidth.ToString()))</font></td>
                                                            }
                                                            else
                                                            {
                                                                <td class="boarderLeft" style="border-right:1px solid black;"> <font>0</font></td>
                                                            }

                                                            @if (x.Routing.CutNo != null)
                                                            {
                                                                <td class="boarderLeft" style="border-right:1px solid black;"><font>@String.Format("{0:#}", float.Parse(x.Routing.CutNo.ToString()))</font></td>
                                                            }
                                                            else
                                                            {
                                                                <td class="boarderLeft" style="border-right:1px solid black;"> <font>0</font></td>
                                                            }

                                                            @if (x.Routing.Trim != null)
                                                            {
                                                                <td class="boarderLeft" style="border-right:1px solid black;"><font>@String.Format("{0:#}", float.Parse(x.Routing.Trim.ToString()))</font></td>
                                                            }
                                                            else
                                                            {
                                                                <td class="boarderLeft" style="border-right:1px solid black;"> <font>0</font></td>
                                                            }
                                                            break;
                                                        }

                                                    }
                                                }
                                                else
                                                {
                                                    <td class="boarderLeft" style="border-right:1px solid black;"> <font>0</font></td>

                                                    <td class="boarderLeft" style="border-right:1px solid black;"> <font>0</font></td>

                                                    <td class="boarderLeft" style="border-right:1px solid black;"> <font>0</font></td>

                                                }

                                                if (String.IsNullOrEmpty(@item.Leng))
                                                {
                                                    <td class="boarderLeft" style="border-right:1px solid black;"><font>0</font></td>
                                                }
                                                else
                                                {
                                                    <td class="boarderLeft" style="border-right:1px solid black;"> <font>@String.Format("{0:#,#}", float.Parse(item.Leng)) </font></td>
                                                }
                                            }
                                            else
                                            {
                                                <td class="boarderLeft" style="border-right:1px solid black;"> <font></font></td>
                                                <td class="boarderLeft" style="border-right:1px solid black;"> <font></font></td>
                                                <td class="boarderLeft" style="border-right:1px solid black;"> <font></font></td>
                                                <td class="boarderLeft" style="border-right:1px solid black;"> <font></font></td>
                                                <td class="boarderLeft"> <font></font></td>
                                            }
                                        }
                                        else
                                        {
                                            <td class="boarderLeft" style="border-right:1px solid black;"> <font></font></td>
                                            <td class="boarderLeft" style="border-right:1px solid black;"> <font></font></td>
                                            <td class="boarderLeft" style="border-right:1px solid black;"> <font></font></td>
                                            <td class="boarderLeft" style="border-right:1px solid black;"> <font></font></td>
                                            <td class="boarderLeft" style="border-right:1px solid black;"> <font></font></td>
                                            <td class="boarderLeft"> <font></font></td>
                                        }
                                    </tr>
                                </tbody>
                            </table>
                            @{
                                if (item.OrderItem == "" || item.OrderItem == null)
                                {
                                    <center style="padding-top:85px;border-top:1px solid black;"></center>
                                }
                                else
                                {
                                    var stratIndex = item.OrderItem.Length - 10;
                                    if (stratIndex <= 0)
                                    {
                                        stratIndex = 0;
                                    }

                                    if (item.OrderItem.Length > 10)
                                    {

                                        var orderItemJsBarcode = userSession.FactoryCode == "214FB-1" || userSession.FactoryCode == "214EB-1" || userSession.FactoryCode == "214GB-1" ? new string(item.OrderItem.Take(10).ToArray()) : @item.OrderItem.Substring(@stratIndex, 10);
                                        <div style="padding-top:3px;padding-bottom:3px;border-top:1px solid black;">
                                            <div class="row text-center">
                                                <div class="col-xs-12" style="padding-top:5px;">
                                                    <svg class="SObarcode" style="height:85px; width:400px;padding:0;margin:0"
                                                         jsbarcode-format="CODE39"
                                                         jsbarcode-value="@orderItemJsBarcode"
                                                         jsbarcode-textmargin="0">
                                                    </svg>
                                                    <script type="text/javascript">
                                                        JsBarcode(".SObarcode").init();
                                                    </script>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                    else
                                    {
                                        <div style="padding-top:3px;padding-bottom:3px;border-top:1px solid black;">
                                            <div class="row text-center">
                                                <div class="col-xs-12" style="padding-top:5px;">
                                                    <svg class="SObarcode" style="height:85px; width:400px;padding:0;margin:0"
                                                         jsbarcode-format="CODE39"
                                                         jsbarcode-value="@item.OrderItem.Substring(stratIndex, item.OrderItem.Length)"
                                                         jsbarcode-textmargin="0">
                                                    </svg>
                                                    <script type="text/javascript">
                                                        JsBarcode(".SObarcode").init();
                                                    </script>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                }
                            }
                        </div>
                    </div>
                </div>

                <table style="margin-right:0;border-bottom:none;" border="1">
                    <tbody>
                        <tr>
                            <td>
                                <table style="width: 622px;" class="tableMO" cellspacing="0">
                                    <tbody>
                                        <tr style="height: 52px;">
                                            <td class="tableMO" colspan="3" style="width: 85px;border-left-color:white;">
                                                <font class="FontTitle">@Localizer["Product Code"]</font>
                                                <div class="text-center">
                                                    <b><font class="FontParagraph text-wrap" style="font-size:26px">@item.PC</font></b>
                                                </div>
                                            </td>
                                            @*85*@
                                            <td class="tableMO" colspan="4" style="width: 85px;border-top-color:white">
                                                <font class="FontTitle">@Localizer["Material No."]</font>
                                                <div class="text-center">
                                                    <b><font class="FontParagraph" style="font-size:30px;letter-spacing:5px">@item.Material_No</font></b>
                                                </div>
                                            </td>
                                            @*85*@
                                        </tr>
                                        <tr style="height: 52px;">
                                            <td class="tableMO" colspan="3" style="width: 85px;border-left-color:white">
                                                <font class="FontParagraph" style="font-size:large">@item.Box_Type</font>
                                                <div>
                                                    <font class="FontParagraph" style="font-size:large">@Localizer["รูปแบบ"] : </font>
                                                    <font class="FontParagraph" style="font-size:large">@item.RSC_Style  &nbsp; @item.CGType</font>
                                                </div>
                                            </td>
                                            <td class="tableMO" colspan="4" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["Part No"].</font>
                                                <div class="text-center">
                                                    <b><font class="FontParagraph" style="font-weight:normal">@item.Part_No</font></b>
                                                </div>
                                            </td>
                                        </tr>

                                        <tr style="height: 43px;">
                                            <td class="tableMO" rowspan="2" style="width: 85px;border-left-color:white">
                                                @if (@item.PalletPath_Base64 != null && @item.PalletPath_Base64 != "")
                                                {
                                                    <div class="frame" style="width: 85px;max-height:100px">
                                                        <img class="imageFrame" src="@item.PalletPath_Base64" />
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="frame" style="width: 85px;max-height:100px">&nbsp;</div>
                                                }
                                            </td>
                                            <td class="tableMO" colspan="2" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["รอยต่อ"] : </font>

                                                @if (item.JoinType != null && item.JoinType.Length > 20)
                                                {
                                                    <b><font class="FontTitle" style="font-weight:normal">@item.JoinType</font></b>
                                                }else{
                                                    <b><font class="FontParagraph" style="font-weight:normal">@item.JoinType</font></b>
                                                }
                                            </td>
                                            <td class="tableMO" colspan="2" style="width: 85px;">
                                                <font class="FontTitle">@Localizer["การพิมพ์"] : </font>
                                                <b><font class="FontParagraph" style="font-weight:normal">@item.Print_Method</font></b>
                                            </td>
                                            <td class="tableMO text-center" colspan="2">
                                                @if (item.GlWid is not null && item.GlWid.Value)
                                                {
                                                    <font class="text-center FontParagraph" style="font-weight:normal">@Localizer["ลิ้นกาวด้านกว้าง"]</font>
                                                }
                                                <font class=" text-center" size="3"></font>
                                            </td>
                                        </tr>

                                        <tr style="height: 52px;">
                                            <td class="tableMO" style="width: 100px;width: 85px;">
                                                <font class="FontTitle">@Localizer["ขนาดกระบะ"]</font>
                                                <div class="text-center">
                                                    @if (item.PalletSize != null && item.PalletSize != "")
                                                    {
                                                        <b><font class="FontParagraph">@item.PalletSize</font></b>
                                                    }
                                                    else
                                                    {
                                                        <font class="FontParagraph">&nbsp;</font>
                                                    }
                                                </div>
                                            </td>
                                            <td class="tableMO" style="width: 100px;width: 85px;">
