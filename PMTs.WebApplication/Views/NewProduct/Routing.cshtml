        function ChangeScoreType(item) {
            var scoreGapsByScoreType = scoreGaps.filter(s => { return s.ScoreType == item });
            const scoreGapsUniqueByKey = [...new Map(scoreGapsByScoreType.map(item =>
                [item["ScoreGap1"], item])).values()];
            let appendStr = "";
            $.each(scoreGapsUniqueByKey, function (cKey, cObj) {
                appendStr = appendStr + "<option value=\"" + cObj.ScoreGap1 + "\">" + cObj.ScoreGap1 + "</option>";
            });
            $(".dropdownScoreGap").empty();
            $(".dropdownScoreGap").append("<input type=\"number\" class=\"form-control\" style=\"padding: 3px\" id=\"modelRouting_ScoreGap\" asp-for=\"modelRouting.ScoreGap\" />");
            $(".dropdownScoreGap").append("<select class=\"form-control\" style=\"padding:0\" onchange=\"this.previousElementSibling.value=this.value; this.previousElementSibling.focus()\">" + appendStr + "</select>");
        }
