@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@model PMTs.DataAccess.ModelView.NewProduct.TransactionDataModel
@{
    ViewData["Title"] = "DetailProduct";
}
@{
    var jointSeparate = "";
    //STD = 1, Non STD1 = 2, Non STD2 = 3
    var typeOfProperty = 0;
    var formGroup = Model.modelCategories.FormGroup;
    if (formGroup == "ROLL" || formGroup == "BOM" || formGroup == "TCG" || Model.modelCategories.Id_MatType == 1)
    {
        typeOfProperty = 2;
    }
    else if (formGroup == "PALLET" || formGroup == "CG" || formGroup == "HB" || formGroup == "HC" || formGroup == "DIGITAL")
    {
        typeOfProperty = 1;
    }
    else if (formGroup == "STDRSC" || formGroup == "FULLRSC" || formGroup == "STDDC" || formGroup == "OFFSETTS" || formGroup == "SF" || formGroup == "SB" || formGroup == "SPCRSC" || formGroup == "SS" || formGroup == "AC")
    {
        typeOfProperty = 0;
    }

    ViewData["TypeOfProperty"] = typeOfProperty;
}
<style type="text/css">
    input[type="text"] {
        /*width: 150px;*/
        display: block;
        border: 0;
        background-color: white;
    }

    input[type="checkbox"] {
        display: block;
        border: 0;
        background-color: white;
    }

    input[readonly], input[readonly="readonly"] {
        background-color: white;
    }

    input:read-only {
        /*background-color: blue;*/
        /*color: white;*/
    }

    .frame {
        height: 160px; /*can be anything*/
        width: 160px; /*can be anything*/
        position: relative;
    }

    .imageFrame {
        max-height: 100%;
        max-width: 100%;
        width: auto;
        height: auto;
        position: absolute;
        top: 0;
        bottom: 0;
        left: 0;
        right: 0;
        /*margin: auto;*/
    }

    .tableMO {
        border-left: 1px solid black;
        border-bottom: 1px solid black;
    }

    /*.divbo {
            height: 100px;
            width: 100px;
            float: none;
            padding-left: 20px;
            padding-bottom : 10px;
            background-color: powderblue;
        }*/

    /*Font style*/
    .FontHeader {
        overflow: hidden;
        text-overflow: ellipsis;
        line-height: 36px; /* fallback */
        max-height: 75px; /* fallback */
        font-size: 32px;
        padding-left: 3px;
    }

    .FontSubHeader1 {
        word-break: break-all;
        max-height: 32px;
        line-height: 34px;
        font-size: 24px;
        padding-left: 3px;
    }

    .FontSubHeader2 {
        word-break: break-all;
