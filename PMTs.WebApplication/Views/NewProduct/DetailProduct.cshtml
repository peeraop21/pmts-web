    $(document).ready(function () {
        $('.tblcontainer1').on('click', '.tblremovetagprint', function () {
            var id = this.id;
            var split_id = id.split("_");
            var deleteindex = split_id[1];
            swal({
                title: "Are you sure ?",
                text: "Do you want delete .",
                icon: "warning",
                buttons: {
                    confirm: "Ok",
                    cancel: true,
                },
                dangerMode: true,
