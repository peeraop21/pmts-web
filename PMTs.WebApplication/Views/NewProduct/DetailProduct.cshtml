     function chkSpcRsc() {
            if ($("#modelProductSpec_Leg").val() == $('#l').val() && $("#modelProductSpec_Wid").val() == $('#w').val() && $("#modelProductSpec_Hig").val() == $('#h').val() && $('#x').val() == $('#modelProductSpec_spcLen').val() && $('#c').val() == $('#modelProductSpec_Code').val())
                ComputeWeightAndArea();
