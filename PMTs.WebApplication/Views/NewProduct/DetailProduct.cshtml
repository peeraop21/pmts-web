                                        @if (Model.modelProductSpec.UnUpgradBoard == true)

                                        {
                                            <input class="filled-in" type="checkbox" value="" id="chkUpg" disabled checked readonly>
                                            <label class="form-check-label" for="chkUpg"></label>
                                        }
                                        else
                                        {
                                            <input class="filled-in" type="checkbox" value="" id="chkUpg" disabled readonly>
                                            <label class="form-check-label" for="chkUpg"></label>
                                        }
                                    </td>
                                </tr>
                                <tr>
                                    <td></td>
                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <div class="form-group row">
                                            <div class="col-sm-12">
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>

                        <div class="form-group row">

                            @if (Model.modelProductSpec.BoardAlt.Count > 0)
                            {
                                <div class="card">
                                    <h4 class="card-title">
                                        <b> @Localizer["Board Alternative "]:</b>
                                    </h4>
