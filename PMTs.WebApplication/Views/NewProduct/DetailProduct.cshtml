                                                    <th>Flute</th>
                                                    <th>Board</th>
                                                    <th>Board KIWI</th>
                                                    <th>Active</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in Model.modelProductSpec.BoardAlt)
